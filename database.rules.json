{
  "rules": {
    ".read": "auth != null",
    ".write": "auth != null",
    "profiles": {
      "$profilesid": {
        "profiles": {
          ".indexOn": ["email"]
        }
      }
    },
    "class-checkins": {
      ".indexOn": ["email", "date"]
    },
    "dance-checkins": {
      ".indexOn": ["email", "date"]
    },
    "dances": {
      ".indexOn": ["date"]
    },
    "monthly-passes": {
      ".indexOn": ["year", "monthName", "email"]
    }
  }
}
