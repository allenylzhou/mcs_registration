{
  "rules": {
    ".read": "auth != null",
    ".write": "auth != null",
    "profiles": {
      ".indexOn": ["email"],
      ".validate": "newData.child('email').val().length > 0"
    },
    "class-checkins": {
      ".indexOn": ["email", "date"]
    },
    "dance-checkins": {
      ".indexOn": ["email", "date"]
    }
  }
}
